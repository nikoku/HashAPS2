{
  "include": [
    "./src/*"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "es2018"
    ],
    "target": "es2018",
    "jsx": "react",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
  }
}